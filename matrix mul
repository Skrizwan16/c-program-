#include<stdio.h>
int main()
{
	int num,m,n;
	printf("enter the starting number =");
	scanf("%d",&m);
	printf("enter the ending number =");
	scanf("%d",&n);
	if(m!=n && m<n && m>0 && n>0){
	for(num=m; num <=n; num++){
	if(num%2==1){
	printf("the odd numbers are =%d\n",num);}}
	printf("\n");
	for(num=m; num <=n;num++) {
	if(num%2==0) {
	printf("the even numbers are =%d\n",num);}}
    }
    else
	{
	printf("invalid input");
}
return 0;
}
